{  
  "manifest_version":2,
  "name":"__MSG_appName__",
  "short_name":"random-user-agent",
  "description":"__MSG_appDesc__",
  "default_locale":"en",
  "version":"1.4",
  "icons":{  
    "16":"img/16x16.png",
    "48":"img/48x48.png",
    "128":"img/128x128.png"
  },
  "author":"samoylovnn@gmail.com",
  "browser_action":{
    "default_icon":"img/48x48.png",
    "default_popup":"popup.html",
    "default_title":"__MSG_appName__"
  },
  "content_scripts":[  
    {  
      "all_frames":true,
      "js":["content.js"],
      "matches":[  
        "http://*/*",
        "https://*/*"
      ],
      "run_at":"document_end"
    }
  ],
  "background":{  
    "scripts":["3rd/randexp/min.js", "background.js"]
  },
  "minimum_chrome_version":"17.0",
  "permissions":["webRequest", "webRequestBlocking", "\u003Call_urls\u003E", "storage"]
}